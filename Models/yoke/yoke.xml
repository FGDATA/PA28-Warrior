<?xml version="1.0" ?> 
  <PropertyList> 
    <path>yoke.ac</path> 
    
 <animation>
  <type>material</type>
    <group-name>Yoke-pa28</group-name>
  <emission>
    <red>0.1</red>
    <green>0.02</green>
    <blue>0.00001</blue>
    <factor-prop>/sim/model/material/instruments/factor</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>translate</type>
  <group-name>Yoke-pa28</group-name>
  <property>fdm/jsbsim/fcs/pitch-trim-sum</property>
  <factor>-0.15</factor>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <group-name>Yoke-pa24</group-name>
  <property>controls/flight/aileron</property>
  <factor>-90.0</factor>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
 </animation>
</PropertyList>
