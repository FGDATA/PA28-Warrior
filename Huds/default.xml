<?xml version="1.0"?>

<PropertyList>
	<name>Default Aircraft Hud</name>
	
 <label>
		<name>Generic</name>
		<x>186</x>
		<y>230</y>
		<halign>left</halign>
		<format>Generic</format>
  </label>

	<label>
		<name>dmem</name>
		<x>-420</x>
		<y>180</y>
		<halign>left</halign>
		<format>DME: %1.1f nm</format>
		<input>
			<property>/instrumentation/dme/indicated-distance-nm</property>
		</input>
</label>

<label>
		<name>dmet</name>
		<x>-360</x>
		<y>180</y>
		<halign>left</halign>
		<format>...%1.1f min</format>
		<input>
			<property>/instrumentation/dme/indicated-time-min</property>
		</input>
</label>



		
	<import>
		<path>Huds/Instruments/turn-bank-indicator.xml</path>
		<y-offset>-195</y-offset>
	</import>

	<import>
		<path>Huds/Sets/coordinates.xml</path>
		<y-offset>190</y-offset>
	</import>

	<import>
		<path>Huds/Instruments/compass.xml</path>
		<y-offset>190</y-offset>
	</import>

	<import>
		<path>Huds/Sets/controls.xml</path>
	</import>

	<import>
		<path>Huds/Sets/parameters.xml</path>
	</import>

	<import>
		<path>Huds/Sets/autopilot.xml</path>
		<x-offset>-100</x-offset>
		<y-offset>160</y-offset>
	</import>

    <import>
		<path>Huds/Sets/gps.xml</path>
		<x-offset>-420</x-offset>
		<y-offset>160</y-offset>
	</import>

	<import>
		<path>Huds/Instruments/ladder.xml</path>
	</import>

	<import>
		<path>Huds/Instruments/runway.xml</path>
	</import>
	
	 <label>
		<name>Wind</name>
		<x>-420</x>
		<y>-200</y>
		<halign>left</halign>
		<format>Wind Speed: %1.2f knots</format>
		<input>
			<property>/environment/wind-speed-kt</property>
		</input>
</label>

 <label>
		<name>From</name>
		<x>-420</x>
		<y>-190</y>
		<halign>left</halign>
		<format>Wind Hdg:    %d deg</format>
		<input>
			<property>/environment/wind-from-heading-deg</property>
		</input>
</label>

<label>
		<name>gs</name>
		<x>-420</x>
		<y>-90</y>
		<halign>left</halign>
		<format>Grd-Speed:     %d kts</format>
		<input>
			<property>/velocities/groundspeed-kt</property>
		</input>
	</label>

		<label>
		<name>rpm</name>
		<x>-420</x>
		<y>-100</y>
		<halign>left</halign>
		<format>Eng Revs:    %d rpm</format>
		<input>
			<property>/engines/engine/rpm</property>
		</input>
	</label>

	<label>
		<name>vs</name>
		<x>-420</x>
		<y>-110</y>
		<halign>left</halign>
		<format>V - Speed:     %d fpm</format>
		<input>
			<property>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
		</input>
	</label>

  <label>
		<name>ADF/NDB</name>
		<x>-420</x>
		<y>-130</y>
		<halign>left</halign>
		<format>Adf/Ndb:   %s</format>
		<input>
			<property>/instrumentation/adf/ident</property>
		</input>
</label>

 <label>
		<name>Nav1-Id</name>
		<x>-420</x>
		<y>-140</y>
		<halign>left</halign>
		<format>Nav1-ID:   %s</format>
		<input>
			<property>/instrumentation/nav/nav-id</property>
		</input>
</label>

 <label>
		<name>Nav2-Id</name>
		<x>-420</x>
		<y>-150</y>
		<halign>left</halign>
		<format>Nav2-ID:   %s</format>
		<input>
			<property>/instrumentation/nav[1]/nav-id</property>
		</input>
</label>



 <label>
		<name>maghead</name>
		<x>-420</x>
		<y>-170</y>
		<halign>left</halign>
		<format>Mag Hdg:  %1.1f deg</format>
		<input>
   	<property>/orientation/heading-magnetic-deg</property>
		</input>
</label>

 <label>
		<name>magvar</name>
		<x>-420</x>
		<y>-160</y>
		<halign>left</halign>
		<format>Mag Var:  %1.1f deg</format>
		<input>
   	<property>/environment/magnetic-variation-deg</property>
		</input>
</label>

<label>
		<name>Fuell</name>
		<width>2</width>
		<height>2</height>
		<x>-420</x>
		<y>-220</y>
		<halign>left</halign>
		<format>Fuel-L: %2.1f gls</format>
		<input>
			<property>/consumables/fuel/tank/level-gal_us</property>
		</input>
	</label>

<label>
		<name>FFlow</name>
		<width>2</width>
		<height>2</height>
		<x>-420</x>
		<y>-230</y>
		<halign>left</halign>
		<format>FFlow: %2.1f gph</format>
		<input>
			<property>/engines/engine/fuel-flow_gph</property>
		</input>
	</label>


  <label>
		<name>FuelR</name>
		<width>2</width>
		<height>2</height>
		<x>-340</x>
		<y>-220</y>
		<halign>left</halign>
		<format>Fuel-R: %2.1f gls</format>
		<input>
			<property>/consumables/fuel/tank[1]/level-gal_us</property>
		</input>
	</label>

   <label>
		<name>odom</name>
		<width>2</width>
		<height>2</height>
		<x>-420</x>
		<y>-210</y>
		<halign>left</halign>
		<format>Flown: %1.1f nm</format>
		<input>
			<property>/instrumentation/gps/trip-odometer</property>
		</input>
	</label>


   <label>
		<name>rem</name>
		<width>2</width>
		<height>2</height>
		<x>-330</x>
		<y>-210</y>
		<halign>left</halign>
		<format>Rem: %1.1f nm</format>
		<input>
			<property>/autopilot/route-manager/distance-remaining-nm</property>
		</input>
	</label>

</PropertyList>



